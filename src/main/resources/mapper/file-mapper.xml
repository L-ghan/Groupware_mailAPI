<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="file">
    <insert id="insert">
        insert into files (id, mailId) values (file_seq.nextval, #{mailId})
    </insert>
</mapper>